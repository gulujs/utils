{
  "name": "@lunjs/utils",
  "version": "0.1.0",
  "description": "utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./type": "./dist/type.js"
  },
  "typesVersions": {
    "*": {
      "interfaces": [
        "dist/interfaces.d.ts"
      ],
      "type": [
        "dist/type.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rimraf ./dist && tsc --project tsconfig.build.json",
    "lint": "eslint --report-unused-disable-directives --ignore-path .gitignore --ext .ts ./src",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunjs/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "author": "jKey Lu <jkeylu@gmail.com> (https://github.com/jkeylu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lunjs/utils/issues"
  },
  "homepage": "https://github.com/lunjs/utils#readme",
  "devDependencies": {
    "@lunjs/eslint-config": "^0.4.4",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
